    $('#single__program_list > ul > li > ul > li').each(function() {
    	console.log(current + ' - ' + $('> a', this).text());
    	if (current == $('> a', this).text()) {
          $('> ul', this).find('li').each(function() {
              index ++; 
              var url = $('> a', this).attr('href'),
                  output = '<li id="single_program__'+index+'" class="single__program_box"><i class="fas fa-circle-notch fa-spin"></i></li>';
			  $('#single__program_output ul.list').attr('data-num', index);
              $('#single__program_output ul.list').append(output);
              $('#single_program__'+index).load(url + ' #program__metadata', function(){
                  $(this).addClass('loaded');
                  $('> #program__metadata', this).attr('id', 'program__metadata_'+uniqid('id'));
