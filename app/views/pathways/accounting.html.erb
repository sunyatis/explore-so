                  var deg = $('> div', this).attr('data-degree');
                  if ($('.filter a[data-filter="'+deg+'"]').length) {
                  	// Do Nothing
                  } else {
                    
                    $('.filter').append('<li><a href="#" data-filter="'+deg+'">'+deg+'</a></li>');
                   
                  }
                  hackerList.reIndex()
                  //see function sorter
				sorter();
  
              });
          });
        }
	});
  
  //Alphabetical sorter for labels above program lists (industry pages, accounting etc..)
  function sorter(){
    var parent = $('.filter');
    var listitems = parent.children('li').get();
    listitems.sort(function(a, b) {
      return $(a).text().toUpperCase().localeCompare($(b).text().toUpperCase());
    })
    
    $.each(listitems, function(idx, itm) { parent.append(itm); });//reappend li to the ul in the new sortedform 
    
  }
   
    hackerList.on('updated', function(){
    	var search = $('#single__program_output .search').val();
        if (search) {
        	$('#single__program_output ul.list:not(.searched)').addClass('searched');
        } else {
        	$('#single__program_output ul.list.searched').removeClass('searched');
        }
    });

	$(document).on('click', '.filter > li > a', function(e) {
    	e.preventDefault();
        var search = $(this).attr('data-filter');
        if (!$(this).hasClass('active')) {
          $('.filter a.active').removeClass('active');
          $(this).addClass('active');
        }

        if (!search || search == 'all') {
        	$('#single__program_output ul.list.filtered').removeClass('filtered');
			hackerList.filter();
        } else {
        	hackerList.filter(function(item) {
        		$('#single__program_output ul.list:not(.filtered)').addClass('filtered');
            	return (item.values().filter__degree == search);
          	});
		}
    });
});

