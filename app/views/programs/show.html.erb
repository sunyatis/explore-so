<% title "#{@program.prog_title} - #{@program.prog_level.titleize} - #{@program.school.abbv}"  %>
<% meta_key "#{@program.meta_keywords}" %>
<% meta_desc "#{@program.meta_desc}" %>
<style>
	#top_rfi{
		display:none;
	}
	</style>
	
<% if @program.open_suny == 'SUNY Online'%>
	<%= render(
	  partial: 'programs/das',
	  locals: { programs: @programs }
	) %>
<% else %>
	<%= render(
	  partial: 'programs/regshow',
	  locals: { programs: @programs }
	) %>
<% end %>

