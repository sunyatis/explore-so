<div id="filterrific_results" class="row">
  <div class="container">
  	<div class="pull-right resets"  data-intro="Reset the filter settings to defaults." data-position="bottom">
    	<%= link_to('Reset filters', reset_filterrific_url, :class => 'reset_link') %>

  	</div>
  	<div class="chardin_box" data-intro="will_paginate's entries info works as expected." data-position="bottom">
 	   <h1> <%= page_entries_info @programs, entry_name: "programs" %></h1>
 	 </div>
 	 <div class="chardin_box" data-intro="Spinner indicates update." data-position="bottom">

        <span class="filterrific_spinner" style="display:none;">
          <%= render_filterrific_spinner %>
        </span>
	 </div>
   </div>
<section class="degree-results row">

	<ul class="results">
		<div class="info-title">
			<dl>
				<dt>
					<%= image_tag('/assets/Blue_Ribbon.svg', :alt => "SUNY Online Signature Programs", :width => '19px', :height => '27px', :id => 'per_icon', :rel => 'tooltip', "data-placement" =>'top', :title => 'SUNY Online Signature Programs') %> 
					<a href="/content/about">
						<span>Signature Program</span>
					</a>
				</dt>
				<dd>
				</dd>
			</dl>
			<dl>
				<dt class="delivery-mode-extra">Delivery Mode</dt>
				<dd>	
				</dd>
			</dl>
			<dl>
				<dt class="degree-type-extra">Degree Type</dt>
				<dd>
					
				</dd>
			</dl>
			<dl>
				<dt class="credit-type-extra">Credits</dt>
				<dd>	
				</dd>
			</dl>
		</div>
		 <% @programs.each do |programs| %>
		<li class="full-width">
			<div class="result">
				<div class="program">
						<figure>
							<div class="logo">
								<%= link_to image_tag(programs.school.image.url(:small), :align=> "center"), programs %>
							</div>
							<figcaption>
								<h2><%= link_to programs.prog_title, programs%></h2>
								<h3><%= programs.school.name %></h3>
							</figcaption>
						</figure>

					<div class="info">
						<dl>
							<dt>
								
							</dt>
							<dd>
						   	 <% if programs.open_suny == "Open SUNY" %>
						          <%= image_tag('/assets/Blue_Ribbon.svg', :alt => "SUNY Online Signature Programs", :width => '20px', :id => 'per_icon', :rel => 'tooltip', "data-placement" =>'top', :title => 'SUNY Online Signature Programs') %> 
						   	   <% elsif programs.open_suny == "SUNY Online" %>
						   	    <%= image_tag('/assets/Blue_Ribbon_SO.svg', :alt => "SUNY Online Degree at Scale", :width => '20px', :id => 'per_icon', :rel => 'tooltip', "data-placement" =>'top', :title => 'SUNY Online Degree at Scale') %> 
						   	       <% end %>
							</dd>
						</dl>
						<dl>
							<dt>Delivery Mode</dt>
							<dd class="extra-margin-delivery"><%= programs.per_courses_online %>% Online</dd>
						</dl>
						<dl>
							<dt>Degree Type</dt>
							<dd class="extra-margin-degree"><%= programs.level_expanded  %></dd>
						</dl>
						<dl>
							<dt>Credits</dt>
							<dd class="extra-margin">12</dd>
						</dl>
					</div>
				</div>

			</div>

	</li> <% end %>
</ul>

</section> 

